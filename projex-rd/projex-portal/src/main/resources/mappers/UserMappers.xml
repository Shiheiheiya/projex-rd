<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.projex.portal.mapper.UserMapper">
    <sql id="user">
        user_id, username, password
    </sql>
    <select id="getUser" resultType="com.projex.portal.vo.UserResult">
        select
        *
        from user
        where user_id = #{userId}
    </select>

</mapper>